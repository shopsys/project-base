msgid ""
msgstr ""
"Content-Type: text/plain; charset=UTF-8\n"
"Content-Transfer-Encoding: 8bit\n"
"Language: cs\n"

msgid "An array of values is not supported as an input for \"slider\" type parameter. Use \"minimalValue\" and \"maximalValue\" instead."
msgstr "Pole hodnot (\"values\") není podporováno jako vstup pro parametry typu \"slider\". Namísto toho použijte \"minimalValue\" and \"maximalValue\"."

msgid "Catalog number cannot be longer than {{ limit }} characters"
msgstr "Katalogové číslo nesmí být delší než {{ limit }} znaků"

msgid "Email cannot be longer than {{ limit }} characters"
msgstr "E-mail nesmí být delší než {{ limit }} znaků"

msgid "Flag color must be in valid hexadecimal code e.g. #3333ff"
msgstr "Barva příznaku musí mít správný formát hexadecimálního kódu, např.: #3333ff"

msgid "Flag name cannot be longer than {{ limit }} characters"
msgstr "Název příznaku nemůže být delší než {{ limit }} znaků"

msgid "Image can be only in JPG or PNG format"
msgstr "Obrázek může být pouze ve formátu JPG nebo PNG"

msgid "Image can be only in JPG, GIF or PNG format"
msgstr "Obrázek může být pouze ve formátu JPG, GIF nebo PNG"

msgid "Minimal and maximal value are not supported as an input for other than \"slider\" type parameter. Use \"values\" instead."
msgstr "Minimální a maximalní hodnota nejsou podporovány jako vstup pro parametry jiného typu než \"slider\". Namísto toho použijte \"values\"."

msgid "New password must be at least {{ limit }} characters long"
msgstr "Nové heslo musí být alespoň {{ limit }} znaků dlouhé"

msgid "Password has to include uppercase letters, lowercase letters, numbers and must be longer than 10 characters."
msgstr "Heslo musí obsahovat velká písmena, malá písmena, číslice a musí být delší než 10 znaků."

msgid "Passwords do not match"
msgstr "Hesla se neshodují"

msgid "Please choose image"
msgstr "Vyberte prosím obrázek"

msgid "Please enter GTM ID"
msgstr "Prosím vyplňte GTM ID"

msgid "Please enter email"
msgstr "Vyplňte prosím e-mail"

msgid "Please enter email content"
msgstr "Vyplňte prosím text e-mailu"

msgid "Please enter flag color"
msgstr "Vyplňte prosím barvu příznaku"

msgid "Please enter flag name in all languages"
msgstr "Prosím zadejte název příznaku ve všech jazycích"

msgid "Please enter new password"
msgstr "Vyplňte prosím nové heslo"

msgid "Please enter notification bar content"
msgstr "Vyplňte prosím obsah notifikační lišty"

msgid "Please enter password"
msgstr "Vyplňte prosím heslo"

msgid "Please enter valid email"
msgstr "Vyplňte prosím platný e-mail"

msgid "Please enter video ID"
msgstr "Prosím vyplňte ID videa"

msgid "Please enter your email address"
msgstr "Vyplňte prosím e-mail"

msgid "Product prefix name cannot be longer than {{ limit }} characters"
msgstr "Prefix produktu nesmí být delší než {{ limit }} znaků"

msgid "Product suffix name cannot be longer than {{ limit }} characters"
msgstr "Suffix produktu nesmí být delší než {{ limit }} znaků"

msgid "Product with entered catalog number already exists"
msgstr "Produkt s tímto katalogovým číslem již existuje"

msgid "Provided hash is not valid."
msgstr "Zadaný hash není validní."

msgid "Uploaded image is to large ({{ size }} {{ suffix }}). Maximum size of an image is {{ limit }} {{ suffix }}."
msgstr "Uploadovaný obrázek je příliš velký ({{ size }} {{ suffix }}). Maximální velikost obrázku je {{ limit }} {{ suffix }}."

msgid "User with provided email address does not exist."
msgstr "Uživatel s uvedenou e-mailovou adresou neexistuje."

msgid "Variable {{ needle }} is required"
msgstr "Proměnná {{ needle }} je povinná"

